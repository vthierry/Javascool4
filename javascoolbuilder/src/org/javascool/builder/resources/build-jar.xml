<?xml version="1.0" encoding="UTF-8"?>
<project name="${jvs.jarname}" default="setup" basedir=".">
    <target name="jar">
        <jar basedir="${jvs.tojar.tmp}" destfile="${jvs.newjar}">
            <manifest>
                <attribute name="Built-By" value="JVSBuilder" />
                <attribute name="Main-Class" value="org.javascool.JVSMain" />
                <attribute name="Class-Path" value="" />
            </manifest>
        </jar>
     <!--//DEBUG   <delete dir="${jvs.tmp}"/>-->
    </target>
    <target name="setup">
        <mkdir dir="${jvs.tojar.tmp}"/>
        <unjar src="${jvs.basejar}" dest="${jvs.tojar.tmp}"/>
    </target>
</project>
